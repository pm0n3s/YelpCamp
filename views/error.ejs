<% layout('layouts/boilerplate') %>
<div class="row">
    <div class="col-lg-6 offset-lg-3">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= err.message %></h4>
            <% if (process.env.NODE_ENV !== 'production') {%>
            <hr>
            <p class="text-break"><%= err.stack %></p>
            <% } %>
        </div>
    </div>
</div>